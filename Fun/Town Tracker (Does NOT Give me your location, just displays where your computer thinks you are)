javascript:(function() {  if (navigator.geolocation) {    navigator.geolocation.getCurrentPosition(function(position) {      var latitude = position.coords.latitude;      var longitude = position.coords.longitude;      fetch(`https://nominatim.openstreetmap.org/reverse?format=jsonv2&lat=${latitude}&lon=${longitude}&zoom=10`)%20%20%20%20%20%20%20%20.then(function(response)%20{%20%20%20%20%20%20%20%20%20%20return%20response.json();%20%20%20%20%20%20%20%20})%20%20%20%20%20%20%20%20.then(function(data)%20{%20%20%20%20%20%20%20%20%20%20var%20city%20=%20data.address.city%20||%20data.address.town%20||%20data.address.village%20||%20%27%27;%20%20%20%20%20%20%20%20%20%20alert(%27%20By%20WatermelonVR:%20Your%20location:%20%27%20+%20city);%20%20%20%20%20%20%20%20})%20%20%20%20%20%20%20%20.catch(function(error)%20{%20%20%20%20%20%20%20%20%20%20console.log(error);%20%20%20%20%20%20%20%20%20%20alert(%27Failed%20to%20retrieve%20location%20data.%27);%20%20%20%20%20%20%20%20});%20%20%20%20});%20%20}%20else%20{%20%20%20%20alert(%27Geolocation%20is%20not%20supported%20by%20your%20browser.%27);%20%20}})();
